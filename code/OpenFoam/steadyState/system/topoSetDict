 x 	 5.142130e-01;  // Length of sample


actions
(
    // wDomain
    {
        name    wDomainCellSet;
        type    cellSet;
        action  new;
        source  boxToCell;
        sourceInfo{
        box     (0 0 0) ($x $x $x);
        }
    }
    {
        name    wDomain;
        type    cellZoneSet;
        action  new;
        source  setToCellZone;
        sourceInfo{
          set     wDomainCellSet;
        }
    }

);



